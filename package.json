{
  "name": "vue-spider-admin",
  "version": "0.0.1",
  "description": "Backend templates for vue",
  "keywords": [
    "vue",
    "vue3",
    "naive",
    "naive-pro",
    "naive-admin",
    "naive-ui",
    "naive-ui-pro",
    "naive-ui-admin"
  ],
  "author": {
    "name": "cnguu",
    "email": "www@cnguu.cn",
    "url": "https://github.com/cnguu"
  },
  "repository": "https://github.com/spider-nest/vue-spider-admin.git",
  "homepage": "https://github.com/spider-nest/vue-spider-admin",
  "bugs": {
    "url": "https://github.com/spider-nest/vue-spider-admin/issues"
  },
  "license": "MIT",
  "main": "./src/main.ts",
  "scripts": {
    "serve": "cross-env --max_old_space_size=4096 vite",
    "build": "vite build",
    "report": "cross-env REPORT=true yarn build",
    "preview": "vite preview",
    "lint:eslint": "eslint \"src/**/*.{vue,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write --loglevel warn \"src/**/*.{js,json,tsx,css,vue,html,md}\"",
    "lint:lint-staged": "lint-staged -c ./.husky/lint-staged.js",
    "lint:pretty": "pretty-quick --staged",
    "clear:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "clear:modules": "rimraf node_modules",
    "clear:install": "rimraf yarn.lock && rimraf node_modules && yarn",
    "install:husky": "is-ci || husky install",
    "postinstall": "yarn install:husky"
  },
  "dependencies": {
    "@vicons/fluent": "^0.10.0",
    "@vicons/utils": "^0.1.4",
    "@vueuse/core": "^5.1.3",
    "axios": "^0.21.1",
    "crypto-js": "^4.0.0",
    "lodash-es": "^4.17.21",
    "mockjs": "^1.1.0",
    "naive-ui": "^2.15.5",
    "pinia": "^2.0.0-beta.5",
    "qrcode": "^1.4.4",
    "qs": "^6.10.1",
    "vfonts": "^0.1.0",
    "vue": "^3.1.4",
    "vue-router": "^4.0.10"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.0.2",
    "@types/mockjs": "^1.0.4",
    "@types/qrcode": "^1.4.1",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "@vitejs/plugin-vue": "^1.2.5",
    "@vitejs/plugin-vue-jsx": "^1.1.6",
    "@vue/compiler-sfc": "^3.1.4",
    "autoprefixer": "^10.3.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-define-config": "^1.0.9",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.13.0",
    "husky": "^6.0.0",
    "is-ci": "^3.0.0",
    "lint-staged": "^11.0.1",
    "postcss": "^8.3.5",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "typescript": "^4.3.5",
    "vite": "^2.4.2",
    "vite-plugin-compression": "^0.3.0",
    "vite-plugin-html": "^2.0.7",
    "vite-plugin-imagemin": "^0.4.0",
    "vite-plugin-mock": "^2.9.1",
    "vue-eslint-parser": "^7.8.0"
  },
  "engines": {
    "node": "^12 || >=14"
  }
}
